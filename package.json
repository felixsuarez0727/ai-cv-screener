{
  "name": "ai-cv-screener",
  "version": "1.0.0",
  "description": "AI-powered CV screening system with RAG pipeline",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "generate-cvs": "node cv-generator/generateCVs.js",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "build": "cd frontend && npm run build",
    "setup": "node setup.js",
    "test": "node test-system.js"
  },
  "keywords": ["ai", "cv", "screener", "rag", "hr"],
  "author": "AI CV Screener",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "fs-extra": "^11.1.1"
  }
} 